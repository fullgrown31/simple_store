<h1>Listing products</h1>

<table>
  <tr>
    <th>Product Title</th>
    <th>Product Price</th>
    <th>Stock Quantity</th>
    <th colspan="3"></th>
  </tr>

  <% @products.each do |product| %>
    <tr>
      <td><%= product.title %></td>
      <td><%= product.price %></td>
      <td><%= product.stock_quantity %></td>
    </tr>
  <% end %>
</table>